<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="KIAMS.Views.Accounts.DashView"
            Shell.NavBarIsVisible="False"
            NavigationPage.HasNavigationBar="False"
             xmlns:pages="clr-namespace:KIAMS.Views.Accounts">
  <FlyoutPage.Flyout>
    <pages:DashViewFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:DashViewDetail />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>